{
    "version": 2,
    "builds": [
      {
        "src": "index.py",
        "use": "@vercel/python"
      },
      {
        "src": "index.tsx",
        "use": "@vercel/next"
      }
    ],
    "routes": [
            {
        "src": "/(.*)",
        "dest": "index.py"
      },
      { "src": "/app/pages(.*)", "dest": "/app/pages$1" }
    ]
  }
  